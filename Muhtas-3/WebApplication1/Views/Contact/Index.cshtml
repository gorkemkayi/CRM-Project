@using WebApplication1.Models.Database
@model List<Contact>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<table class="table table-striped">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Position</th>
        <th>E Mail</th>
        <th>Edit</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Position</td>
            <td>@item.Email</td>
            <td><a class="btn btn-success" href="/Contact/EditContact/@item.ID">Edit</a></td>
        </tr>
    }
</table>


